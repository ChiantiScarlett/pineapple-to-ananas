{
  "name": "Pineapple to Ananas",
  "description": "Changes pineapple to ananas.",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["<all_urls>"],
  "browser_action": {
    "default_title": "Change pineapple to ananas.",
    "default_icon": "icon-32px.png"
  },
  "icons": {
    "32": "icon-32px.png",
    "300": "icon-300px.png"
  },
  "manifest_version": 2
}
